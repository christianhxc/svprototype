CREATE  OR REPLACE VIEW `view_vih_regiones_per` AS
    SELECT 
        (CASE
            WHEN
                ((`r`.`id_region` = 1)
                    AND (`per`.`sexo` = 'M'))
            THEN
                SUM(1)
            ELSE SUM(0)
        END) AS `region_m1`,
